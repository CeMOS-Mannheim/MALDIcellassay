# data prep #####
data <- read_excel("Y:/17-Thomas Enzlein/Paper contributions/Petit 2021/figure 1B for thomas.xlsx",
sheet = "Tabelle1")
aoo <- read_excel("Y:/17-Thomas Enzlein/Paper contributions/Petit 2021/Sara_AAO_Data.xlsx",
sheet = "AOO")
lvls <- c("WT", "L166P", "G384A", "I143T", "Ins113T", "R278T", "L166R", "G266S", "L171P", "L282R",
"E280A", "V89L", "F105C", "A246E", "R269H", "I143F", "A79V", "Y115H", "M139V", "M139T", "M233I",
"M233V", "L381F", "L235P", "L235V", "A431E", "R35Q", "E318G")
lvls_abc <- c(lvls[1],sort(lvls[c(-1, -26:-28)]), lvls[26:28])
data %>%
tidyr::gather(Substrate, val, -Species) %>%
filter(!is.na(val)) %>%
filter(!str_detect(val, ",")) %>%
mutate(val = as.numeric(val)) %>%
filter(!is.na(val)) %>%
separate(Substrate, sep = "-", into = "Substrate") %>%
group_by(Substrate, Species) %>%
summarise(mean = mean(val)) %>%
mutate(norm = mean/sum(mean)*100,
Species = fct_recode(Species,
"Ab37" = "AB 37",
"Ab38" = "AB 38",
"Ab40" = "AB 40",
"Ab42" = "AB 42",
"Ab43" = "AB 43")) %>%
ungroup() %>%
mutate(Substrate = factor(Substrate, levels = lvls_abc)) -> df
# Abeta profile plot ####
ggplot(df, aes(x = Substrate, y= norm, fill = Species)) +
geom_col(position = "stack") +
scale_fill_discrete(type = c("#0000FF", "#FF6000", "#00C000", "#AD07E3", "#808080")) + # pretty ugly...
theme_pubclean()+
theme(text = element_text(size = 20, face = "bold"),
axis.text.x = element_text(angle = 45, hjust = 1),
legend.position = "top") +
labs(fill = "",
x = "",
y = expression(Ab[n]~ "as % total Ab")) -> prof_p
prof_p
df %>%
select(-mean) %>%
spread(Species, norm) -> df2
df3 <- df2
df2 %>%
gather(Species, norm, -Substrate) %>%
left_join(aoo %>%
